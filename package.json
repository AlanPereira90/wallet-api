{
  "name": "wallet-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon",
    "dev": "nodemon",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts' --exit",
    "test:tdd": "npm run test -- --watch --watch-extensions ts",
    "test:coverage": "rimraf ./coverage && nyc npm run test",
    "lint": "tsc --project tsconfig.json --noEmit && eslint ./src/**/*.ts --fix",
    "format": "prettier --write .",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "cd migrations && typeorm migration:create ",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/domain/infra/PostgresConnection.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/domain/infra/PostgresConnection.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:AlanPereira90/wallet-api.git"
  },
  "author": "AlanPereira90",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlanPereira90/wallet-api/issues"
  },
  "homepage": "https://github.com/AlanPereira90/wallet-api#readme",
  "dependencies": {
    "@aws-sdk/util-dynamodb": "^3.58.0",
    "aws-sdk": "^2.1109.0",
    "body-parser": "^1.20.0",
    "cores": "^0.8.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/response-time": "^2.3.5",
    "@types/sinon-chai": "^3.2.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "easy-api-doc": "^0.2.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "test/**",
      "node_modules/**/*",
      "**/*.d.ts",
      "dist/**/*",
      "*.js",
      "src/index.ts",
      "src/application/setup/**/*",
      "src/domain/config/**/*",
      "src/application/config/**/*",
      "src/domain/utils/environment.ts",
      "src/domain/utils/ResponseError.ts",
      "src/domain/infra/DynamoDB.ts",
      "src/application/middlewares/errorHandler.ts"
    ],
    "all": true,
    "cache": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "per-file": false
  }
}
